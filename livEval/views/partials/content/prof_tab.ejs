<div class="row">
    <div class="col-md-12">
        <ul class="nav nav-pills">
            <li role="presentation" class="active">
                <a href="#questionnaire" data-toggle="tab" role="tab">Questionnaire</a
            </li>
            <li >
                <a href="#visio" data-toggle="tab" role="tab">Visio</a>
            </li>
        </ul>
        <div class="tab-content">
            <div role="tabpanel" id="questionnaire" class="tab-pane fade active in">
                <div class="col-md-8">
                    <div id="form_questionnaire" class="panel panel-primary">
                        <div class="panel-heading">
                            <h3>Créer un questionnaire pour votre évaluation </h3>
                        </div>
                        <div class="panel-body">
                            <form class="form-horizontal">
                                <fieldset>
                                    <!-- Select Basic -->
                                    <div class="form-group col-md-6">
                                        <label class="col-md-4 control-label" for="nbquestion">Nombre de questions</label>
                                        <div class="col-md-4">
                                            <select ng-model="nbquestion" id="nbquestion" name="nbquestion" class="form-control">
                                                <option ng-repeat="n in range(0, 10)" value="{{n}} ">{{n}} </option>
                                            </select>
                                        </div>
                                    </div>
                                    <ul class="col-md-6">
                                        <li ng-repeat="student in etudiantList">
                                            <strong>
                                                {{student.name.last | uppercase}} {{student.name.first | uppercase}}
                                                <button class="btn btn-primary">
                                                    Ajouter
                                                </button>
                                            </strong>
                                        </li>
                                    </ul>
                                </fieldset>

                                <fieldset>

                                <!-- Form Name -->
                                <legend>Questions / Réponses</legend>

                                <!-- Text input-->
                                <div class="animate-repeat" ng-repeat="n in range(1, nbquestion)">
                                    <div class="form-group">
                                      <label class="col-md-4 control-label" for="question">question {{n}} </label>  
                                      <div class="col-md-5">
                                      <input id="question" name="question" type="text" placeholder="questions de l'évaluation" class="form-control input-md">
                                      </div>
                                    </div>

                                    <!-- Textarea -->
                                    <div class="form-group">
                                      <label class="col-md-4 control-label" for="reponse">réponse {{n}}</label>
                                      <div class="col-md-4">                     
                                        <textarea class="form-control" id="reponse" name="reponse">écrivez la ou les réponses éventuelles que vous attendez de l'étudiant</textarea>
                                      </div>
                                    </div>
                                    <hr class="rule2">
                                </div>
                                

                                </fieldset>
                            </form>
                        </div>
                    </div>
                </div>
                <div class="col-md-4">
                    <h1>liste des étudiants</h1>
                    <div class="panel panel-info">
                      <div class="panel-heading">Etudiants inscits à vos cours</div>
                      <div class="panel-body">
                        <ul>
                            <li>etudiant</li>
                            <li>etudiant</li>
                            <li>etudiant</li>
                            <li>etudiant</li>
                            <li>etudiant</li>
                            <li>etudiant</li>
                            <li>etudiant</li>
                            <li>etudiant</li>
                            <li>etudiant</li>
                            <li>etudiant</li>
                        </ul>
                      </div>
                    </div>
                </div>
            </div>
            <div role="tabpanel" id="visio" class="tab-pane fade active in">
                <h1>Visio</h1>
                <video autoplay></video>
            </div>
        </div>
    </div>

    

    <div class="col-md-6">
        
    </div>
</div>